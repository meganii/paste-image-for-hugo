{
	"name": "paste-image-for-hugo",
	"displayName": "Paste image for Hugo",
	"description": "Paste image from Clipboard",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.pasteImage4Hugo"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": [
			{
				"type": "object",
				"title": "Configuration for pasteImage4Hugo",
				"properties": {
					"pasteImage4Hugo.cloudinaryName": {
						"type": "string",
						"default": "",
						"description": "Cloudinary cloud_name"
					},
					"pasteImage4Hugo.cloudinaryAPIKey": {
						"type": "string",
						"default": "",
						"description": "Cloudinary API KEY"
					},
					"pasteImage4Hugo.cloudinaryAPISecret": {
						"type": "string",
						"default": "",
						"description": "Cloudinary API Secret"
					}
				}
			}
		],
		"commands": [
			{
				"command": "extension.pasteImage4Hugo",
				"title": "PasteImage4Hugo"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "yarn run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^8.10.25",
		"tslint": "^5.8.0",
		"typescript": "^3.1.4",
		"vscode": "^1.1.25"
	},
	"dependencies": {
		"cloudinary": "^1.13.2",
		"moment": "^2.23.0",
		"node-ipc": "^9.1.1"
	}
}
